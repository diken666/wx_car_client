<view class='container'>
  <view class='h-image'>
    <image src='../../image/register.png' mode='widthFix'></image>
  </view> 
  <form bindsubmit='register'>
    <view class='f-item'>
      <image src='../../image/userLogin.png'></image>
      <input type='text' name='account' placeholder='账号'></input>
    </view>
    <view class='f-item'>
        <image src='../../image/psw.png'></image>
        <input type='text' name='password' placeholder='密码'></input>
    </view>
    <view class='f-item'>
      <image src='../../image/name.png'></image>
      <input type='text' name='name' placeholder='姓名'></input>
    </view>
    <view class='f-item'>
      <image src='../../image/gender.png'></image>
      <radio-group name="sex">
        <label><radio value="男"/>男</label>
        <label><radio value="女"/>女</label>
      </radio-group>
    </view>
    <view class='f-item'>
      <image src='../../image/location.png'></image>
      <input type='text' name='location' placeholder='地址'></input>
    </view>
    <view class='f-item'>
      <image src='../../image/phone.png'></image>
      <input type='number' name='phone' placeholder='电话'></input>
    </view>
    <button class='btn-register' form-type='submit'>注册</button>
  </form>
</view>